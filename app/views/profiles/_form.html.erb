<%= simple_form_for(@profile) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :firstName %>
    <%= f.input :lastName %>
    <%= f.file_field :linkPhoto %>
    <!-- <%= f.input :linkPhoto %> -->
    <%= f.input :linkGoogle %>
    <%= f.input :dob, start_year: Date.today.year - 16, end_year: Date.today.year - 60%>
    <!-- <%= f.association :user %> -->
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
